{
    "name": "Weather Report",
    "version": "1.0.0",
    "description": "Provides real time weather data on various major locations. Includes a button to change between Celsius and Farenheit.",
    "manifest_version": 3,
    "author": "Hdjekso",
    "icons":{
        "32": "assets/icon32.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
    },
    "action":{
        "default_icon":{
            "32": "assets/icon32.png",
            "48": "assets/icon48.png",
            "128": "assets/icon128.png"
        },
        "default_popup": "index.html",
        "default_title": "Real-time Weather Report"
    }
}